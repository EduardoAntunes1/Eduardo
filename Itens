package Principal;

public class Itens {
	private String nome;
	private Double preco;
	private Integer quantidade;
	
	public Itens(String nome, Integer quantidade, Double preco) {
		this.nome = nome;
		this.preco = preco;
		this.quantidade = quantidade;
	}
	
	public String getNome() {
		return nome;
	}
	
	public Integer getQuantidade() {
		return quantidade;
	}
	
	public Double getPreco() {
		return preco;
	}

}
